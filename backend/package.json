{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern '**/*.spec.ts' --ignore-pattern '**/*.integration.spec.ts'",
    "typeCheck": "tsc --noEmit",
    "start:dev": "dotenv -e .env.development -- nest start --watch",
    "start:debug": "dotenv -e .env.development -- nest start --debug --watch",
    "generate:gqlschema": "dotenv -e .env.development -- ts-node --transpile-only -r tsconfig-paths/register src/main.ts",
    "start:prod": "dotenv -e .env.production -- prisma migrate deploy && dotenv -e .env.production -- node dist/main",
    "migrate:create": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate deploy",
    "migrate:test": "dotenv -e .env.test -- prisma migrate deploy",
    "migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "migrate:reset": "dotenv -e .env.development prisma migrate reset",
    "prisma:generate": "npx dotenv -e .env.development -- npx prisma generate",
    "test:integration": "dotenv -e .env.test -- jest --config ./test/integration/jest-integration-config.json --runInBand",
    "test:unit": "dotenv -e .env.test -- jest --config ./test/unit/jest-unit-config.json",
    "codegen": "graphql-codegen --config graphql-codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.2.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "^11.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@prisma/client": "^5.22.0",
    "apollo-server-errors": "^3.3.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cross-fetch": "^4.1.0",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@golevelup/ts-jest": "^1.1.1",
    "@graphql-codegen/cli": "6.1.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-resolvers": "5.1.5",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.10",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "pg": "^8.16.3",
    "prettier": "^3.4.2",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  }
}
